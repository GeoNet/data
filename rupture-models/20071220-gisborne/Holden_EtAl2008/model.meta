# TOML format -------------------------------------------------------------
# Contains Finite Source Parameters meta data 
# -------------------------------------------------------------------------
[model]
 citation  = '[Holden et al.(2008)]'
 reference = 'Fran√ßois-Holden, C., Bannister, S., Beavan, J., Cousins, J., Field, B., McCaffrey, R., McVerry, G., Reyners, M., Ristau, J. and Samsonov, S. (2008). "The Mw 6.6 Gisborne earthquake of 2007: Preliminary records and general source characterisation". Bulletin of the New Zealand Society for Earthquake Engineering, 41(4), 266-277, https://doi.org/10.5459/bnzsee.41.4.266-277.'
 
[event]
