 Nz        = nan
 Dx        = nan
 Dz        = nan
 Ntw       = nan
 SHF       = nan
 Fmin      = 0.01
 Fmax      = 0.10
 SVF       = 'Unknown'
 LEN       = nan

[inversion.data]
